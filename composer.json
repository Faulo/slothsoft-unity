{
	"name" : "slothsoft/unity",
	"description" : "Slothsoft's Unity API",
	"license" : "WTFPL",
	"authors" : [{
			"name" : "Daniel Schulz",
			"email" : "info.slothsoft@gmail.com"
		}
	],
	"type" : "library",
	"require" : {
		"php" : "7.2 - 8.0",
		"slothsoft/core" : "^1.3",
		"slothsoft/farah" : "^1.3",
		"symfony/process" : "^5.0",
		"symfony/filesystem" : "^5.0",
		"mustangostang/spyc" : "~0.6"
	},
	"require-dev" : {
		"phpunit/phpunit" : "8.5.*"
	},
	"autoload" : {
		"psr-4" : {
			"Slothsoft\\Unity\\" : "src"
		},
		"files" : [
			"scripts/bootstrap.php"
		]
	},
	"keywords" : [
		"slothsoft",
		"docs",
		"unity"
	],
	"autoload-dev" : {
		"psr-4" : {
			"Slothsoft\\Unity\\" : "tests"
		}
	},
	"extra" : {
		"branch-alias" : {
			"dev-develop" : "1.2.x-dev"
		}
	},
	"bin" : [
		"bin/unity-daemon"
	],
	"scripts" : {
		"farah-asset" : "@php vendor/slothsoft/farah/scripts/farah-asset.php",
        "unity-tests" : "@php scripts/tests.php",
        "unity-daemon" : "@php scripts/daemon.php"
	},
	"config" : {
		"process-timeout" : 0
	},
	"homepage" : "http://farah.slothsoft.net/modules/unity"
}