{
	"name" : "slothsoft/unity",
	"description" : "Slothsoft's Unity API",
	"license" : "MIT",
	"authors" : [{
			"name" : "Daniel Schulz",
			"email" : "info.slothsoft@gmail.com"
		}
	],
	"type" : "library",
	"require" : {
		"php" : ">=7.4",
		"slothsoft/core" : "^1.10",
		"slothsoft/farah" : "^1.12.5",
		"symfony/process" : "^5.0",
		"symfony/filesystem" : "^5.0",
		"mustangostang/spyc" : "0.6.2",
		"mintopia/vdfkeyvalue" : "~1.0",
		"symfony/dom-crawler" : "^5.4",
		"symfony/browser-kit" : "^5.4",
		"symfony/http-client" : "^5.4",
		"symfony/mime" : "^5.4",
		"vlucas/phpdotenv" : "^5.6",
		"php-imap/php-imap" : "^5.0"
	},
	"require-dev" : {
		"phpunit/phpunit" : "^8.5.40"
	},
	"autoload" : {
		"psr-4" : {
			"Slothsoft\\Unity\\" : "src"
		},
		"files" : [
			"scripts/bootstrap.php"
		]
	},
	"keywords" : [
		"slothsoft",
		"docs",
		"unity"
	],
	"autoload-dev" : {
		"psr-4" : {
			"Slothsoft\\Unity\\" : "tests"
		}
	},
	"extra" : {
		"branch-alias" : {
			"dev-main" : "2.18.x-dev"
		}
	},
	"bin" : [
		"scripts/autoversion",
		"scripts/steam-buildfile",
		"scripts/unity-build",
		"scripts/unity-documentation",
		"scripts/unity-help",
		"scripts/unity-method",
		"scripts/unity-start",
		"scripts/unity-module-install",
		"scripts/unity-package-install",
		"scripts/unity-project-setting",
		"scripts/unity-project-version",
		"scripts/unity-tests",
		"scripts/transform-dotnet-format"
	],
	"config" : {
		"process-timeout" : 0
	},
	"homepage" : "http://farah.slothsoft.net/modules/unity"
}